{
  "problemMatcher": [
      {
          "owner": "jest-reporter",
          "pattern": [
              {
                  "regexp": "^(.*)\\s(\\d+):(\\d+)\\s(Error): ([\\w\\s\\d:.<>()/-]*)%%%%%$",
                  "file": 1,
                  "line": 2,
                  "column": 3,
                  "severity": 4,
                  "message": 5
              }
          ]
      }
  ]
}
