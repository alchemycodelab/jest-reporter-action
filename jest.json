{
  "problemMatcher": [
      {
          "owner": "jest-reporter",
          "pattern": [
              {
                  "regexp": "^(.*)\\s(\\d+):(\\d+) (\\d)$",
                  "file": 1,
                  "line": 2,
                  "column": 3,
                  "severity": 4,
                  "code": 5
              }, {
                  "regexp": "^([\\w\\s\\d:.<>()/-]*)%%%%%$",
                  "message": 1,
                  "loop": true
              }
          ]
      }
  ]
}
